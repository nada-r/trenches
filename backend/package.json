{
  "name": "trenches-back",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint src/ --ext .ts",
    "lint:tests": "npx eslint spec/ --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "PORT=80 nodemon",
    "console": "npx ts-node src/console.ts",
    "prisma:gen": "npx prisma generate",
    "prisma:pull-prod": "dotenv -e .env.production -- npx prisma db pull",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "dotenv:pull": "npx dotenv-vault@latest pull",
    "dotenv:push": "npx dotenv-vault@latest push",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@metaplex-foundation/umi-uploader-irys": "^0.10.0-beta.0",
    "@prisma/client": "^5.18.0",
    "@types/cors": "^2.8.17",
    "@types/find-config": "^1.0.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "dotenv-vault": "^1.26.2",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "find-config": "^1.0.0",
    "helmet": "^7.1.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "prisma": "^5.18.0",
    "ts-command-line-args": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@inquirer/prompts": "^5.5.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.2.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-plugin-n": " ^17.10.2",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.2.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma-json-types-generator": "^3.0.4",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "zod-prisma-types": "^3.1.8"
  }
}
